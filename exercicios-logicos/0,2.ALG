algoritmo "0,2"
//nome:murillo dorsa
var
    numero:inteiro
    numero1:inteiro
    resultado:inteiro
inicio
    escreval("Digite um numero: ")
    leia(numero)
    
    escreval("digite outro numero: ")
    leia(numero1)
    
    resultado <- numero - numero1
    
    escreval("seu resultado é: ",resultado)

fimalgoritmo
