algoritmo "0,4"
//nome:murillo dorsa
var
    numero:real
    numero1:real
    resultado:real
inicio
    escreval("Digite um numero: ")
    leia(numero)
    
    escreval("digite outro numero: ")
    leia(numero1)
    
    resultado <- numero / numero1
    
    escreval("seu resultado é: ",resultado)

fimalgoritmo
