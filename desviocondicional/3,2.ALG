algoritmo "3,2"
//nome:murillo dorsa
var
   A:inteiro
  B:inteiro



inicio

    
     escreva("Digite o valor de A: ")
    leia(A)
    escreva("Digite o valor de B: ")
    leia(B)

    // Comparação entre A e B
    se A > B então
        escreva("A é maior que B")
    senao
        escreva("B é maior que A")
    fimse

fimalgoritmo
